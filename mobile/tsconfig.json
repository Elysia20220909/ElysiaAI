{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
	"exclude": ["node_modules"]
}
