{
	"name": "elysia-ai",
	"version": "1.0.50",
	"license": "MIT",
	"description": "Elysia(Bun) + FastAPI + Milvus Lite + Ollama によるRAG AIチャット",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chloeamethyst/ElysiaJS.git"
	},
	"homepage": "https://github.com/chloeamethyst/ElysiaJS",
	"bugs": {
		"url": "https://github.com/chloeamethyst/ElysiaJS/issues"
	},
	"keywords": [
		"Elysia",
		"Bun",
		"RAG",
		"Ollama",
		"Milvus",
		"FastAPI",
		"AI"
	],
	"author": "chloeamethyst",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "bun run --watch src/index.ts",
		"build": "webpack --config webpack.config.js --mode=production",
		"start": "node dist/index.js",
		"pack:zip": "powershell -NoProfile -Command \"if(Test-Path dist.zip){Remove-Item dist.zip -Force}; Compress-Archive -Path dist/* -DestinationPath dist.zip -Force\"",
		"prepublishOnly": "bun run build",
		"lint": "biome check .",
		"format": "biome format --write .",
		"fix": "biome check --write ."
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.74",
		"@elysiajs/html": "^1.4.0",
		"@elysiajs/static": "^1.4.7",
		"@zilliz/milvus2-sdk-node": "^2.6.5",
		"ai": "^5.0.104",
		"alpinejs": "^3.15.2",
		"axios": "^1.13.2",
		"elysia": "latest",
		"htmx.org": "^2.0.8",
		"ollama": "^0.6.3",
		"ollama-ai-provider": "^1.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/node": "^24.10.1",
		"bun-types": "latest",
		"ts-loader": "^9.5.4",
		"typescript": "^5.9.3",
		"webpack": "^5.103.0",
		"webpack-cli": "^6.0.1",
		"webpack-node-externals": "^3.0.0"
	},
	"main": "dist/index.js",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"module": "src/index.js"
}
