{
  "name": "elysia-ai",
  "version": "1.0.51",
  "license": "MIT",
  "description": "Elysia(Bun) + FastAPI + Milvus Lite + Ollama によるRAG AIチャット",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chloeamethyst/ElysiaAI.git"
  },
  "homepage": "https://github.com/chloeamethyst/ElysiaAI",
  "bugs": {
    "url": "https://github.com/chloeamethyst/ElysiaAI/issues"
  },
  "keywords": [
    "Elysia",
    "Bun",
    "RAG",
    "Ollama",
    "Milvus",
    "FastAPI",
    "AI"
  ],
  "author": "chloeamethyst",
  "scripts": {
    "test": "bun test",
    "test:local": "powershell -ExecutionPolicy Bypass -File ./scripts/test-local.ps1",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "dev": "node -e \"process.platform==='win32'?require('child_process').exec('powershell -ExecutionPolicy Bypass -File ./scripts/dev.ps1'):require('child_process').exec('bash ./scripts/dev.sh')\"",
    "dev:win": "powershell -ExecutionPolicy Bypass -File ./scripts/dev.ps1",
    "dev:unix": "bash ./scripts/dev.sh",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force dist, .tsbuildinfo, *.log -ErrorAction SilentlyContinue\"",
    "clean:deep": "powershell -ExecutionPolicy Bypass -File ./scripts/clean.ps1 -Deep",
    "clean:check": "powershell -ExecutionPolicy Bypass -File ./scripts/clean.ps1 -WhatIf",
    "build": "webpack --config config/internal/webpack.config.js --mode=production",
    "start": "bun src/index.ts",
    "start:all": "powershell -ExecutionPolicy Bypass -File ./scripts/start-all.ps1",
    "chat": "bun dev/chat.ts",
    "love": "bun dev/chat.ts",
    "cui": "bun dev/chat.ts",
    "chat:dev": "bun --watch dev/chat.ts",
    "setup:dev": "bash ./scripts/setup-dev.sh",
    "setup:prod": "sudo bash ./scripts/setup-production.sh",
    "setup:python": "bash ./scripts/setup-python.sh",
    "setup:ssh": "sudo bash ./scripts/ssh-setup.sh",
    "start:fastapi": "bash ./scripts/start-fastapi.sh",
    "start:server": "bun start-server.ts",
    "rotate:jsonl": "bash ./scripts/rotate-jsonl.sh",
    "pack:zip": "powershell -NoProfile -Command \"if(Test-Path dist.zip){Remove-Item dist.zip -Force}; Compress-Archive -Path dist/* -DestinationPath dist.zip -Force\"",
    "prepublishOnly": "npm run build",
    "lint": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format --check .",
    "fix": "biome check --write .",
    "db:init": "bun run scripts/init-prisma.ts",
    "db:migrate": "bun run scripts/prisma-migrate.ts",
    "db:setup": "bun scripts/setup-database.ts",
    "db:seed": "bun run scripts/seed.ts",
    "db:reset": "bunx prisma migrate reset --force",
    "db:studio": "bunx prisma studio",
    "dev:server": "bun scripts/dev-server.ts",
    "dev:test": "bun scripts/dev-test-simple.ts",
    "docker:build": "docker build -f config/docker/Dockerfile.production -t elysia-ai:latest .",
    "docker:up": "docker-compose -f config/docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f config/docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f config/docker/docker-compose.yml logs -f",
    "aws:deploy": "cd cloud/aws && bash deploy.sh",
    "gcp:deploy": "cd cloud/gcp && bash deploy.sh"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/html": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.7",
    "@elysiajs/stream": "^1.1.0",
    "@elysiajs/swagger": "^1.3.1",
    "@libsql/client": "^0.15.15",
    "@node-redis/client": "^1.0.6",
    "@prisma/adapter-libsql": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@types/cron": "^2.4.3",
    "@types/ws": "^8.18.1",
    "@zilliz/milvus2-sdk-node": "^2.6.7",
    "ai": "^6.0.3",
    "alpinejs": "^3.15.2",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.0",
    "bun-promptx": "^0.2.0",
    "chart.js": "^4.5.1",
    "cron": "^4.4.0",
    "dotenv": "^17.2.3",
    "elysia": "^1.4.19",
    "htmx.org": "^2.0.8",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "ollama": "^0.6.3",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^6.10.0",
    "prisma": "^7.1.0",
    "redis": "^5.10.0",
    "sanitize-html": "^2.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@elysiajs/eden": "^1.4.5",
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.2",
    "@types/nodemailer": "^7.0.4",
    "@types/sanitize-html": "^2.16.0",
    "bun-types": "^1.3.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "module": "src/index.js"
}
